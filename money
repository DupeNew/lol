-- This uses the modern task library to run the first script
task.spawn(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/DupeNew/lol/refs/heads/main/l.lua"))() 
end)

-- This uses the modern task library to run the second script
task.spawn(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/DupeNew/lol/refs/heads/main/g.lua"))()
end)
